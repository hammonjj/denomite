<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Audit Logs</title>
  <link rel="stylesheet" href="/static/css/admin.css">
</head>
<body>
  <header>
    <h1>View Audit Logs</h1>
    <a href="/admin">Back to Admin Panel</a>
  </header>
  <table>
    <thead>
      <tr>
        <th>Timestamp</th>
        <th>Action</th>
        <th>Details</th>
      </tr>
    </thead>
    <tbody>
      <% it.logs.forEach(log => { %>
      <tr>
        <td><%= log.timestamp %></td>
        <td><%= log.action %></td>
        <td><%= log.details || 'No details' %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
